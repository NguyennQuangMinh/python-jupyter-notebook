    "from math import *\n",
    "\n",
    "#kiem tra so chinh phuong\n",
    "\n",
    "def square(n):\n",
    "    can = isqrt(n)\n",
    "    if can **2 == n:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "if __name__ == '__main__':\n",
    "    a = int(input())\n",
    "    if square(a):\n",
    "        print('Yes')\n",
    "    else:\n",
    "        print('No')"
