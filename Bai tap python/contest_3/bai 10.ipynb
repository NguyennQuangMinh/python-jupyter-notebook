   "source": [
    "n = int(input())\n",
    "tong_chan = 0\n",
    "tong_le = 0\n",
    "tong = 0\n",
    "for i in range(1, n+1):\n",
    "    if i%2 == 0:\n",
    "        tong_chan += i\n",
    "    else:\n",
    "        tong_le += -i\n",
    "tong = tong_chan + tong_le\n",
    "print(tong)"
   ]
