    "from math import *\n",
    "n = int(input())\n",
    "sum = 0\n",
    "for i in range(1, isqrt(n) + 1):\n",
    "    if n%i == 0:\n",
    "        sum += i\n",
    "        if i != n//i:\n",
    "            sum += n//i\n",
    "print(sum)"
