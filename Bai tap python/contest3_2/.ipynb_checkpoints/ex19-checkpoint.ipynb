    "# in ra những số fibonacci nhỏ hơn hoặc bằng n (dãy bắt đầu bằng số 1)\n",
    "if __name__ =='__main__':\n",
    "    n = int(input())\n",
    "    fn2 = 1\n",
    "    fn1 = 1\n",
    "    for i in range(1, n+1):\n",
    "        fn = fn1 + fn2\n",
    "        if fn >= n:\n",
    "            print(fn)\n",
    "            break\n",
    "        fn2, fn1 = fn1, fn"
