    "from math import *\n",
    "\n",
    "def check(n):\n",
    "    for i in range(0, n//111 + 1):\n",
    "        if (n - i * 111)%11==0:\n",
    "            return True\n",
    "    return False\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "    if check(n):\n",
    "        print('Yes')\n",
    "    else:\n",
    "        print('NO')"
