   "source": [
    "# dùng đệ quy tính tổng của S(n) = 1+2+3+4+5+...+n\n",
    "# cần xác định bài toán con nhỏ nhất và công thức truy hồi \n",
    "def S(n):\n",
    "    if n==0:\n",
    "        return 0\n",
    "    return n + S(n-1)\n",
    "if __name__ == '__main__':\n",
    "    a = int(input())\n",
    "    print(S(a))"
   ]
