    "# đếm số lượng chữ số của n sử dụng đệ quy\n",
    "\n",
    "def S(n):\n",
    "    if n < 10:\n",
    "        return 1\n",
    "    else:\n",
    "        return 1 + S(n//10)\n",
    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "    print(S(n))"
   ]
