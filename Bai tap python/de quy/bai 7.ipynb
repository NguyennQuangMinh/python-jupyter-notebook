    "# bai 7 đổi hệ thập phân sang hệ thập lục phân\n",
    "def F(n):\n",
    "    if n!=0:\n",
    "        F(n//16)\n",
    "        if n%16 < 10:\n",
    "            print(n%16, end='')\n",
    "        else:\n",
    "            print(chr(n%16 + 55), end ='')\n",
    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "    F(n)"
   ]
