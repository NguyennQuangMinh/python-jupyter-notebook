    "#bai 8 tính tổng các chữ số của n\n",
    "def S(n):\n",
    "    if n < 10:\n",
    "        return n\n",
    "    else:\n",
    "        return n%10 + S(n//10)\n",
    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "    print(S(n))"
   ]
