    "\n",
    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "    a = list(map(int, input().split()))\n",
    "    res = 10**18\n",
    "    a.sort()\n",
    "    for i in range(1, n):\n",
    "        res = min(res, a[i] - a[i-1])\n",
    "    print(res)"
