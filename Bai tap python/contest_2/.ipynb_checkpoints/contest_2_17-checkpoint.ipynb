    "# A->Z : 65->90\n",
    "# a->z : 97->142\n",
    "# 0->9 : 48->57\n",
    "c = input()\n",
    "tmp = ord(c)\n",
    "if tmp>=65 and tmp <=90:\n",
    "    print('UPPER')\n",
    "elif tmp>=97 and tmp<=142:\n",
    "    print('LOWER')\n",
    "elif tmp>=48 and tmp<=57:\n",
    "    print('DIGIT')\n",
    "else:\n",
    "    print('SPECIAL')"
   ]
