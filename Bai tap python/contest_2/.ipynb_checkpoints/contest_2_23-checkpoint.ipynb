    "n,m = map(int, input().split())\n",
    "min_step, max_step = 0,n\n",
    "if n%2==0:\n",
    "    min_step = n//2\n",
    "else:\n",
    "    min_step = n//2+1\n",
    "ans = (min_step+m-1)//m*m   # số nhỏ nhất tính từ min_step là bội của m \n",
    "if ans<=max_step:\n",
    "    print(ans)\n",
    "else:\n",
    "    print(-1)"
   ]
